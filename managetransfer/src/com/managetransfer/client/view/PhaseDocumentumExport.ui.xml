<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Grid>
			<g:row>
				<g:customCell>
					<g:Label text="Source Object"/>
				</g:customCell>
				<g:customCell>
					<g:ListBox  ui:field="sourceObjectList"/>
				</g:customCell>
				<g:customCell>
					<g:Label text="Connection"/>
				</g:customCell>
				<g:customCell>
					<g:ListBox ui:field="connectionList"/>
				</g:customCell>
				
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Create Folder "/>
				</g:customCell>
				<g:customCell>
					<g:CheckBox ui:field="createFolder"/>
				</g:customCell>
				<g:customCell>
					<g:Label text="Export Location"/>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="exportLocation" />
				</g:customCell>
			</g:row>
			
		</g:Grid>
		<g:HorizontalPanel width="475px" verticalAlignment="ALIGN_MIDDLE">
			<g:Label text="Export Query" width="81px"/>
			<g:TextBox ui:field="exportQuery" width="384px" height="61px" />
		</g:HorizontalPanel>
		<g:HorizontalPanel width="475px" verticalAlignment="ALIGN_MIDDLE">
			<g:Label text="Repeating Attribute DQL" width="81px"/>
			<g:TextBox ui:field="repeatAttributeDQL" width="384px" height="61px" />
		</g:HorizontalPanel>
		 
		
	</g:HTMLPanel>
</ui:UiBinder> 