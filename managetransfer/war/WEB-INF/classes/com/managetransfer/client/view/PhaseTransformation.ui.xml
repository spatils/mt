<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Grid>
			<g:row>
				<g:customCell>
					<g:Label text="Transformation Name"/>
				</g:customCell>
				<g:customCell>
					<g:ListBox  ui:field="transformationList"/>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 