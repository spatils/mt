<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.managetransfer.businessobject.ProcessVariable2"
		table="mtprocessvariable2">
		<meta attribute="class-description">
			This class contains bill related process
			variables
		</meta>
		<id name="taskId" type="string" column="task_id" length="16">
		</id>
		<property name="form_benefit_state" column="form_benefit_state"
			type="string" />
		<property name="dqlupdateobj" column="dqlupdateobj" type="int" />
		<property name="dcn" column="dcn" type="string" />
		<property name="contenttype" column="contenttype" type="string" />
		<property name="adjuster_assigned" column="adjuster_assigned"
			type="string" />
		<property name="diff_department" column="diff_department"
			type="boolean" />
		<property name="checkcomments" column="checkcomments" type="string" />
		<property name="mitchellcomments" column="mitchellcomments"
			type="string" />
		<property name="start_updatemetadata_flag" column="start_updatemetadata_flag"
			type="string" />
		<property name="updatemetadata_comments" column="updatemetadata_comments"
			type="string" />
		<property name="is_duplicate" column="is_duplicate" type="boolean" />
		<property name="CommentsTemp" column="Comments_temp" type="string" />

	</class>
</hibernate-mapping>