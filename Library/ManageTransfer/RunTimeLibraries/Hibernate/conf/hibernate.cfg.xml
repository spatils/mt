   <property name="hibernate.connection.driver_class">oracle.jdbc.driver.OracleDriver</property>
   <property name="hibernate.bytecode.use_reflection_optimizer">true</property> 

   <!-- Assume test is the database name -->
   <property name="hibernate.connection.url">jdbc:oracle:thin:@dbd001ecl.selective.com:1521:DECM</property>
   <property name="hibernate.connection.username">reports</property>
   <property name="hibernate.connection.password">reports11</property>
   
   
   <!-- List of XML mapping files -->
      <mapping resource="information.hbm.xml"/>
      <mapping resource="claims.hbm.xml"/>
