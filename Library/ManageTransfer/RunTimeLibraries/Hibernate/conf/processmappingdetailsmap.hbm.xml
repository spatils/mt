<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.managetransfer.client.ProcessMappingDetailsMap" table="mtprocessmappingdetailsmap" lazy="false">
      <meta attribute="class-description">
         This class contains the Process mapping details for SDT and Activities
      </meta>
      <composite-id >
          <!-- Mapping Name is the name of the Process Name mapping -->
         <key-property name="sourceProcessName" column="source_process_name" type="string" ></key-property>
         <key-property name="sequenceNumber" column="sequence_number" type="int"></key-property>
         <key-property name="sourceObject" column="source_object" type="string"></key-property>
	     <key-property name="targetObject" column="target_object" type="string"></key-property>
      </composite-id>
      <!-- Mapping type can be ACTIVITY , SDT, VARIABLE,PROCESS -->
      <property name="mappingType" column="mapping_type" type="string"  />
     </class>
</hibernate-mapping>